{
    "presets": [
        "env",
        "es2015",
        "es2017",
        "react",
        "stage-0"
    ],
    "plugins": [
        ["react-css-modules", {
            "webpackHotModuleReloading": true,
            "context": "./"
        }],
        "react-hot-loader/babel",
        "transform-class-properties",
        "syntax-async-functions",
        "transform-regenerator", ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }]
    ]
}